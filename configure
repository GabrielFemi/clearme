#!/usr/bin/env php
<?php

if (file_exists(__DIR__.'/vendor/autoload.php')) {
    require __DIR__.'/vendor/autoload.php';
} else {
    require __DIR__.'/../vendor/autoload.php';
}

$app = new Symfony\Component\Console\Application('ClearMe Setup', '1.0.0');
$app->add(new App\Console\Commands\NewCommand);

$app->run();
